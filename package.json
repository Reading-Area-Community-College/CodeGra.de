    "webpack-merge": "^4.1.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "transform": {
      ".*\\.js$": "babel-jest",
      ".*\\.vue$": "vue-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!vue-awesome|vue-multiselect|vue-resize-split-pane)"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "setupFiles": [
      "<rootDir>/test/unit/setup.js"
    ],
    "collectCoverageFrom": [
      "**/src/**/*.{vue,js}",
      "!**/node_modules/**",
      "!**/docs/**",
      "!**/unknown"
    ]
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
